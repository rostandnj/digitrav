{% extends 'client/base/base.html.twig' %}

{% block main %}
    <div class="wrapper compact-wrapper">

        {% include 'client/dashboard_technician_nav.html.twig' %}



        <div class="main-panel " style="margin-top: -65px">
            <div class="content">
                <div class="panel-header bg-primary-gradient">
                    <div class="page-inner py-5">
                        <div class="d-flex align-items-left align-items-md-center flex-column flex-md-row">
                            <div>
                                <h2 class="text-white pb-2 fw-bold">{{ 'dashboard'|trans }}</h2>

                            </div>

                            <div class="ml-md-auto py-2 py-md-0">
                                <a href="{{ path('web_dashboard_job') }}" class="btn btn-white btn-border btn-round mr-2"><i class="fas flaticon-shopping-bag"></i> {{ 'manage_job'|trans }}</a>
                                <a href="#" class="btn btn-white btn-border btn-round mr-2"><i class="fas fa-building"></i> {{ 'transactions'|trans }}</a>
                                <a href="#" class="btn btn-white btn-border btn-round mr-2"><i class="fas flaticon-alarm"></i> {{ 'notifications'|trans }}</a>

                            </div>

                        </div>
                    </div>
                </div>
                <div class="dash page-inner mt--5">
                    <div class="row row-card-no-pd mt--2">
                        <div class="col-sm-6 col-md-3">
                            <div class="card card-stats card-round">
                                <div class="card-body ">
                                    <div class="row">
                                        <div class="col-5">
                                            <div class="icon-big text-center">
                                                <i class="flaticon-shopping-bag text-warning"></i>
                                            </div>
                                        </div>
                                        <div class="col-7 col-stats">
                                            <div class="numbers">
                                                <p class="card-category">{{ 'jobs'|trans }}</p>
                                                <h6 style="font-size: 14px" class="card-title">{{ data.done }}</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-3">
                            <div class="card card-stats card-round">
                                <div class="card-body ">
                                    <div class="row">
                                        <div class="col-5">
                                            <div class="icon-big text-center">
                                                <i class="flaticon-coins text-success"></i>
                                            </div>
                                        </div>
                                        <div class="col-7 col-stats">
                                            <div class="numbers">
                                                <p class="card-category">{{ 'gains'|trans }}</p>
                                                <h4 style="font-size: 14px" class="card-title">XAF {{ data.earning }}</h4>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-3">
                            <div class="card card-stats card-round">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-5">
                                            <div class="icon-big text-center">
                                                <i class="flaticon-shopping-bag text-danger"></i>
                                            </div>
                                        </div>
                                        <div class="col-7 col-stats">
                                            <div class="numbers">
                                                <p class="card-category">{{ 'active_jobs'|trans }}</p>
                                                <h4 style="font-size: 14px" class="card-title">{{ data.active }}</h4>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-3">
                            <div class="card card-stats card-round">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-5">
                                            <div class="icon-big text-center">
                                                <i class="flaticon-shopping-bag text-primary"></i>
                                            </div>
                                        </div>
                                        <div class="col-7 col-stats">
                                            <div class="numbers">
                                                <p class="card-category">{{ 'waiting_jobs'|trans }}</p>
                                                <h4 style="font-size: 14px" class="card-title">{{ data.awaiting }}</h4>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                {% include 'client/base/dashboard_footer.html.twig' %}
            </div>
        </div>



    </div>



{% endblock %}

{% block footer %}

{% endblock %}


{% block js %}

{% endblock %}
